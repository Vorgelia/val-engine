#pragma Bind Debug
#pragma Bind Time

//This function runs every frame
function StateUpdate(int frame) : int
	switch(frame)
		case 21
		  character_restartState()
		case 0->4
		  character_setFrame("fritz_idle_01")
		  break
		case 5->10
		  character_setFrame("fritz_idle_02")
		  break
		case 11->15
		  character_setFrame("fritz_idle_03")
		  break
		case 16->20
		  character_setFrame("fritz_idle_04")
		  break
	end
	return 0
end